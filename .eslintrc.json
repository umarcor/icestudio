{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "iprof": false,
    "IceProfiler": false,
    "subModuleActive": false,
    "ICEpm": false,
    "IcePlugManager": false,
    "IceEventBus": false,
    "IceTemplateSystem": false,
    "IceParametricHelper": false,
    "Mustache": false,
    "jexcel": false,
    "nw": false,
    "Snap": false,
    "mina": false,
    "$": false,
    "angular": false,
    "console": false,
    "alertify": false,
    "joint": false,
    "_": false,
    "svgPanZoom": false,
    "Backbone": false,
    "async": false,
    "gettext": false,
    "ace": false,
    "g": false,
    "V": false
  },
  "rules": {
    "no-bitwise": 2,
    "camelcase": 2,
    "curly": 2,
    "eqeqeq": 2,
    "wrap-iife": [2, "any"],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-use-before-define": 0,
    "new-cap": 2,
    "no-caller": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "strict": [2, "function"],
    "no-multi-str": 2
  }
}
